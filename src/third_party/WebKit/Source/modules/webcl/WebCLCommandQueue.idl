typedef unsigned long CLenum;
typedef boolean CLboolean;
typedef unsigned long CLuint;

typedef unsigned long CLuint;

[
    Conditional=WEBCL,
] interface WebCLCommandQueue {

    [StrictTypeChecking, Custom, RaisesException] void getInfo(CLenum queuenfo);

    [RaisesException] void enqueueWriteBuffer(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            [EnforceRange] CLuint bufferOffset,
            [EnforceRange] CLuint bufferSize,
            ArrayBufferView ptr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBuffer(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            [EnforceRange] CLuint bufferOffset,
            ImageData srcPixels,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBuffer(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            [EnforceRange] CLuint bufferOffset,
            HTMLImageElement srcImage,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

   [RaisesException] void enqueueWriteBuffer(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            [EnforceRange] CLuint bufferOffset,
            HTMLCanvasElement srcCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBufferRect(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            [EnforceRange] CLuint hostRowPitch,
            [EnforceRange] CLuint hostSlicePitch,
            ArrayBufferView ptr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBufferRect(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            ImageData srcPixels,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBufferRect(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            HTMLCanvasElement srcCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteBufferRect(WebCLBuffer bufferId,
            CLboolean blockingWrite,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            HTMLImageElement srcImage,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueReadBuffer(WebCLBuffer buffer,
            CLboolean blockingRead,
            [EnforceRange] CLuint bufferOffset,
            [EnforceRange] CLuint numBytes,
            ArrayBufferView hostPtr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueReadBufferRect(WebCLBuffer buffer,
            CLboolean blockingRead,
            sequence<CLuint> bufferOrigin,
            sequence<CLuint> hostOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            [EnforceRange] CLuint hostRowPitch,
            [EnforceRange] CLuint hostSlicePitch,
            ArrayBufferView hostPtr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

   [RaisesException] void enqueueReadImage(WebCLImage image,
            CLboolean blockingRead,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            [EnforceRange] CLuint hostRowPitch,
            ArrayBufferView hostPtr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueNDRangeKernel(WebCLKernel kernel,
            [StrictTypeChecking, EnforceRange] CLuint workDim,
            [Default=Undefined] sequence<CLuint> offsets,
            sequence<CLuint> globalWorkSize,
            [Default=Undefined] sequence<CLuint> localWorkSize,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteImage(WebCLImage image,
            CLboolean blockingWrite,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            [EnforceRange] CLuint hostRowPitch,
            ArrayBufferView hostPtr,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteImage(WebCLImage image,
            CLboolean blockingWrite,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            ImageData srcPixels,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteImage(WebCLImage image,
            CLboolean blockingWrite,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            HTMLCanvasElement srcCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWriteImage(WebCLImage image,
            CLboolean blockingWrite,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            HTMLImageElement srcImage,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);


    [RaisesException] void enqueueCopyBuffer(WebCLBuffer sourceBuffer,
            WebCLBuffer targetBuffer,
            [EnforceRange] CLuint sourceOffset,
            [EnforceRange] CLuint targetOffset,
            [EnforceRange] CLuint sizeInBytes,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueCopyBufferRect(WebCLBuffer sourceBuffer,
            WebCLBuffer targetBuffer,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint sourceRowPitch,
            [EnforceRange] CLuint sourceSlicePitch,
            [EnforceRange] CLuint targetRowPitch,
            [EnforceRange] CLuint targetSlicePitch,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueCopyImage(WebCLImage sourceImage,
            WebCLImage targetImage,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueCopyImageToBuffer(WebCLImage sourceImage,
            WebCLBuffer targetBuffer,
            sequence<CLuint> sourceOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint targetOffset,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueCopyBufferToImage(WebCLBuffer sourceBuffer,
            WebCLImage targetImage,
            [EnforceRange] CLuint sourceOffset,
            sequence<CLuint> targetOrigin,
            sequence<CLuint> region,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueWaitForEvents(sequence<WebCLEvent> eventWaitList);

    [RaisesException] void enqueueBarrier();

    [RaisesException] void enqueueMarker(WebCLEvent event);

    [RaisesException] void finish();

    [RaisesException] void flush();

    [RaisesException] void enqueueReadBuffer(WebCLBuffer bufferId,
            CLboolean blockingRead,
            [EnforceRange] CLuint bufferOffset,
            [EnforceRange] CLuint numBytes,
            HTMLCanvasElement dstCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueReadBufferRect(WebCLBuffer buffer,
            CLboolean blockingRead,
            sequence<CLuint> bufferOrigin,
            sequence<CLuint> dstOrigin,
            sequence<CLuint> region,
            [EnforceRange] CLuint bufferRowPitch,
            [EnforceRange] CLuint bufferSlicePitch,
            HTMLCanvasElement dstCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueReadImage(WebCLImage image,
            CLboolean blockingRead,
            sequence<CLuint> origin,
            sequence<CLuint> region,
            HTMLCanvasElement dstCanvas,
            [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
            [Default=Undefined] optional WebCLEvent event);

#if defined(ENABLE_WEBGL) && ENABLE_WEBGL
    [RaisesException] void enqueueAcquireGLObjects(sequence<WebCLMemoryObject> memObjects,
        [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
        [Default=Undefined] optional WebCLEvent event);

    [RaisesException] void enqueueReleaseGLObjects(sequence<WebCLMemoryObject> memObjects,
        [Default=Undefined] optional sequence<WebCLEvent> eventWaitList,
        [Default=Undefined] optional WebCLEvent event);
#endif

#if defined(LANGUAGE_JAVASCRIPT) && LANGUAGE_JAVASCRIPT
    void release();
#endif
};
